1)
#include <stdio.h>

int main()
{
    int i;
    printf("Os 5 primeiros multiplos de 3 sao:\n");
    for(i=1; i<=5; i++)
    {
        printf("%d \n", 3*i);
    }
    return 0;
}

2)
Com For:
#include <stdio.h>

int main()
{
    int i;
    printf("1 a 100:\n");
    for(i=1; i<=100; i++)
    {
        printf("%d \n",i);
    }
    return 0;
}

Com While:
#include <stdio.h>

int main()
{
    int i;
    printf("\n1 a 100:\n");
    i=1;
    while(i<=100)
    {
        printf("%d \n", i);
        i++;
    }
    return 0;
}

Com do-While:
#include <stdio.h>

int main()
{
    int i;
    printf("\n1 a 100: \n");
    do{
        printf("%d \n", i);
        i++;
    }while(i<=100);
    return 0;
}

3)
#include <stdio.h>

int main()
{
    int i=10;
    while(i>=0)
    {
        printf("%d \n", i);
        i--;
    }
    printf("FIM!");
    return 0;
}

