        m+=num;
        i++;
    }
    printf("A media eh: %f", m/10);
    return 0;
}

7)
#include <stdio.h>

int main()
{
    int i=1, num;
    float m;
    while(i<=10)
    {
        printf("Digite o %do numero inteiro para media: ", i);
        scanf("%d", &num);
        if(num<0)
        {
            i++;
        }else{
        m+=num;
        i++;}
    }
    printf("A media foi: %f", m/10);
    return 0;
}

8)
#include <stdio.h>

int main()
{
    int i=2;
    float num, nmaior, nmenor;
    printf("Digite o 1o numero: ");
    scanf("%f", &num);
    nmenor = num;
    nmaior = num;
    while(i <= 10){
        printf("Digite o %do numero: ", i);
        scanf("%f", &num);
        if(num < nmenor){
            nmenor = num;
        }else if(num > nmaior){
            nmaior = num;
        }
        i++;
    }
    printf("O menor numero eh: %f, e o maior eh: %f", nmenor, nmaior);
    return 0;
}

9)
#include <stdio.h>

int main()
{
    int num, i;
    printf("Digite um numero para conferir quantos impares possui:\n");
    scanf("%d", &num);
    for(i=0; i < num; i++)
    {
        printf("%.2d\n", 1+(2*i));
    }
    return 0;
}

