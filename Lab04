11)
#include <stdio.h>

int main()
{
    float i, num;
    printf("Digite um numero natural e vamos imprimir de 0 ate o numero escolhido:\n");
    scanf("%f", &num);
    for(i = 0; i = num; i++){
        printf("\n %.2f", i);
    }
    return 0;
}

12)
#include <stdio.h>

int main()
{
    int i, num, resto, soma=0;
    printf("Digite um numero natural e iremos calcular soma dos seus divisores:\n");
    scanf("%d", &num);
    for(i=1; i<num; i++){
        resto = num%i;
        if(resto == 0){
            soma+=i;
        }
    }
    printf("A soma total eh: %.2d", soma);
    return 0;
}

13)
#include <stdio.h>

int main()
{
    int i, soma=0;
    for(i=1; i<1000; i++){
        if(i%3==0 || i%5==0){
            soma+=i;}
    }
    printf("A soma de todos numeros naturais multiplos de 3 e 5 abaixo de 1000 eh: %d", soma);
    return 0;
}

14)
#include <stdio.h>
#include <stdlib.h>
#include <math.h>

int main()
{
    int i, x, d1, d2;
    printf("Digite a quantidade de vezes que os dois dados devem ser lancados:\n");
    scanf("%d", &x);
    for(i=1; i<=x; i++){
        d1= (rand()%6)+1;
        d2= (rand()%6)+1;
        printf("Lancamento %d:\n", i);
        printf("Valor dado 1: %d, valor dado 2: %d \n", d1, d2);
        if(d1>d2){
            printf("Dado 1 > dado 2 \n");
        }else if(d1==d2){
            printf("Dado 1 = dado 2 \n");
        }else{
            printf("Dado 2 > dado 1 \n");
        printf("\n");}
    }
    return 0;
}

15)
#include <stdio.h>
#include <math.h>
int main()
{
    float num;
    do
    {
        printf("Digite um numero para calcular o quadrado, o cubo e a raiz quadrada(caso seja zero ou menor, a entrada de dados serÃ¡ encerrada):\n");
        scanf("%f", &num);
        printf("\nO quadrado eh: %.2f", num*num);
        printf("\nO cubo eh: %.2f", num*num*num);
        printf("\nA raiz quadrada eh: %f\n", sqrt(num));
    }
    while(num>=0);
    return 0;
}

16)
#include <stdio.h>

int main()
{
    int n1, n2, op;
    do{
        printf("\nCALCULADORA \n");
        printf("Escolha um opcao: \n");
        printf("1. Adicao \n");
        printf("2. Subtracao \n");
        printf("3. Multiplicacao \n");
        printf("4. Divisao \n");
        printf("5. Sair \n");
        printf("Opcao:\n");
        scanf("%d", &op);
        if(op>=1 && op<=4){
            printf("Digite o primeiro valor:\n");
            scanf("%d", &n1);
            printf("Digite o segundo valor:\n");
            scanf("%d", &n2);
        }if(op==1){
            printf("A soma eh: %d\n", n1+n2);
        }else if(op==2){
            printf("A subtracao eh: %d\n", n1-n2);
        }else if(op==3){
            printf("A multiplicacao eh: %d\n", n1*n2);
        }else if(op==4){
            printf("A divisao eh: %.4f\n", ((float) n1/n2));
        }else if(op==5){
            printf("Ate logo!");
        }else{
            printf("Opcao invalida. \n");}
    }while(op!=5);
    return 0;
}

17)
#include <stdio.h>

int main()
{
    float Chico=1.5, Ze=1.1;
    int a=1;
    while (Chico>Ze){
        Chico+=0.02;
        Ze+=0.03;
        ++a;}
    printf("Sera necessario %d anos para Ze ser maior que Chico.", a);
    return 0;
}

18)
include <stdio.h>
#include <math.h>
int main(){
    int a;
    double au, s=2000;
    au = s*0.015;
    for(a=1996; a<=2022; a++){
        s+= au;
        au*= 2;}
    printf("O salario eh: %.2lf", s);
    return 0;
}

19)
#include <stdio.h>    
    
int main()
{
    int v, cem, cinquenta, vinte, dez, cinco, dois, um;
    printf("Digite o valor do saque:\n");
    scanf("%d", &v);
    cem=v/100;
    v%=100;
    cinquenta=v/50;
    v%=50;
    vinte=v/20;
    v%=20;
    dez=v/10;
    v%=10;
    cinco=v/5;
    v%=5;
    dois=v/2;
    v%=2;
    um=v/1;
    printf("Notas R$100,00 = %d \n",cem);
    printf("Notas R$ 50,00 = %d \n",cinquenta);
    printf("Notas R$ 20,00 = %d \n",vinte);
    printf("Notas R$ 10,00 = %d \n",dez);
    printf("Notas R$  5,00 = %d \n",cinco);
    printf("Notas R$  2,00 = %d \n",dois);
    printf("Notas R$  1,00 = %d",um);
    return 0;
}

20)
#include <stdio.h>

int main(){
 int i, j, a, b, soma=0, np=0;
     for(i=1; i<2000000; i++){
       np=1;
       for(j = 2; j <= i; j++){
           if(((i % j) == 0) && (j != i)){
               np = 0;
               break;}
       }if(np!=0 && i!=1){
           soma+=i;}
     }
   printf("A soma de todos numeros primos menores que 2000000 eh: %d", soma);
 return 0;
}

21)
#include <stdio.h>

int main(){
 int i, j, a, b, t=0, np=0, iv=0;
    printf("Digite dois numeros para verificar quantos numeros primos esxistem entre eles:\n");
    scanf("%d%d", &a, &b);
    if(a>=0 && a<b){
     for(i=a+1; i<b; i++){
       np=1;
       for(j = 2; j <= i; j++){
           if(((i % j) == 0) && (j != i)){
               np = 0;
               break;}
       }
       if(np!=0 && i!=1){
           t++;}
     }
   }else{
        printf("Numero invalido!");
        iv=1;}
   if(!iv){
        printf("Existe %d numero(s) primo(s) entre %.2d e %.2d", t, a, b);
   }
 return 0;
}

22)
#include <stdio.h>

int main()
{
    int i=999, j, n, t, ni, im, jm, mnc=0;
    for(n=i*i; n>mnc; n=i*i){
        for(j=i; n>mnc; n=i*j){
            ni=0;
            for(t=n; t>0; t/=10){
                ni=ni*10+t%10;}
            if (n==ni){
                    mnc=n;
                    im=i;
                    jm=j;}
            j--;}
        i--;}
    printf("O maior palindromo de 3 digitos eh: %d.Os numeros que resultaram no produto foi %d x %d\n", mnc, im, jm);
    return 0;
}

23)
#include <stdio.h>

int main()
{
    int v, l=1, sup=1, num=1;
    printf("Digite um valor para construir o Triangulo de Floyd: ");
    scanf("%d", &v);
    while (l<=v){
        while(l>=sup){
            printf("%d ", num);
            sup++;
            num++;
        }
        printf("\n");
        sup=1;
        l++;
    }
    return 0;
}
